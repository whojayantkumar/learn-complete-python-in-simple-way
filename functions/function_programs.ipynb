      "120\n",
      "6\n"
     ]
    }
   ],
   "source": [
    "def factorial(num):\n",
    "    fact=1\n",
    "    while num>=1:\n",
    "        fact=fact*num\n",
    "        num=num-1\n",
    "    return fact\n",
    "print(factorial(5))\n",
    "print(factorial(3))\n",
    "        "
   ]
